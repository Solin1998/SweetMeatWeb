#ppshow{
        width:798px;
		height:250px;
		
        display:flex;
		flex-wrap:wrap;
		justify-content:space-between;
		padding-top:50px;
		}
#ppshow .show1,.show2,.show3,.show4{
         width:190px;
		height:250px;
		overflow:hidden;
		position:relative;
		border-radius:20px;}
#ppshow img{
        width:189px;
		height:250px;
		
		transition:all 1.5s;
		}
#ppshow img:hover{
        transform:scale(1.1) ;
       }

#ppshow .haddle{
          width:189px;
		  height:30px;
		  background:red;
		  color:#fff;
		  display:none;
		  z-index:3;
		  text-align:center;
		  font-size:12px;
		  line-height:30px;
		  }
#ppshow .show1:hover >.haddle{
        position:absolute;
        display:block;
        opacity:0.5;
       }
#ppshow .show2:hover >.haddle{
        position:absolute;
        display:block;
        opacity:0.5;
       }
#ppshow .show3:hover >.haddle{
        position:absolute;
        display:block;
        opacity:0.5;
       }
#ppshow .show4:hover >.haddle{
        position:absolute;
        display:block;
        opacity:0.5;
       }
