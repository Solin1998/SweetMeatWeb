
body{
    background:url(png/bj.jpg) fixed  ;
	background-size:100%;
	 }

section{ 
       
		clear:both;
		height:740px;
		width:960px;
		margin:0 auto;
		background:#fff;
		border-radius:20px;}
aside,#banner{
              float:left;}
aside{
      height:720px;
	  width:160px;
	  background:#666;
	  border-radius:20px;
	  }
aside ul{
          margin:0;padding:0;}
aside li{
         list-style:none;
		 width:160px;
		 height:50px;
		 text-align:center;
		 line-height:50px;
		 box-sizing:border-box;
		 border-radius:20px;
		 }
aside li a{
	display:block;
    font-family:"幼圆";}
aside li:hover>a{
          
		  color:yellow;
		  margin-top:-2px;
		  }
aside li:hover{
          background:#999;
		  border-top:2px solid #fff;
		  border-bottom:2px solid #fff;}
#banner{
        width:700px;
		height:400px;
		position:relative;
		border-radius:20px;
		margin:0 auto;
		overflow:hidden;
		margin-left:50px;}

#banner a{
          position:absolute;
		  top:170px;
		  width:30px;
		  height:75px;
		  background:#000;
		  border-radius:5px;
		  opacity:0.2;
		  text-align:center;
		  line-height:75px;}
#banner a:hover{
            opacity:0.3;}
#banner .a1{
            left:70px;
		   }
#banner .a2{
            right:20px; }
#banner .round{
                margin:0 ;
				padding:0;
				padding-top:20px;
				padding-left:20px;
				width:3100px;
				
				margin:0 auto;
}
#banner .round li{
                  
                 list-style:none;
				 float:left;
}
#banner .round li img{
                  /*height:390px;*/
				width:700px;
}
#banner .round .liheader{margin-left:30px;}
#banner p{clear:both;}
#banner .point{
           margin:0;padding:0;
           width:200px;
		   height:30px;
		   margin:0 auto;
		   margin-top:400px;
		   list-style:none;
		   border-radius:19px;
		   background:#000;
		   opacity:0.2;
		   position:absolute;;
		   top:0px;
		   }
#banner .point:hover{
            opacity:0.3;}
#banner .point li{
            float:left;
		    width:15px;
			height:15px;
			border-radius:50%;
			margin-left:15px;
			margin-top:6px;
			background:#fff;
			z-index:1px;}
#banner .point li:hover{
            background:red;}

#rside {
        width:60px;
		height:350px;
		background:#202020;
		position:fixed;
		right:50px;
		top:150px;
		z-index:10;}
#rside img{
        width:30px;
		margin-left:7px;
		margin-top:10px;
		position:absolute;}
#rside div{
        width:50px;
		height:50px;
        margin:0 auto;
		
		top:50px;
		box-sizing:border-box;
	    border:3px solid transparent;
		}

#rside div:hover{
         border:3px solid #fff;}

#rside div{
	position:relative;
	
}
#rside span.hidden1, span.hidden2,span.hidden3,
span.hidden4,span.hidden5{
               height:45px;
			   width:100px;
			   background:#202020;
			   position:absolute;
			   left:-108px;
			   top:-3px;
			   border:3px solid #FFF;
			   color:#fff;
			   display:none;
			   line-height:45px;
			   text-align:center;
			   }
